#CACHE{86400}
<BOUCLE_principale(ARTICLES) {id_article}>
<!DOCTYPE html>
<html lang="#LANG">>
<head>
<title>[(#TITRE|textebrut) - ][(#NOM_SITE_SPIP|textebrut)]</title>
[<meta name="description" content="(#TITRE|couper{150}|textebrut)" />]
<INCLURE{fond=inc-head}>
[(#REM) RSS della rubrica]
<link rel="alternate" type="application/rss+xml" title="<:syndiquer_rubrique:>" href="[(#URL_PAGE{backend} parametre_url{id_rubrique,#ID_RUBRIQUE})]" />
</head>

<body>
	<INCLURE{fond=inc-cookie}>

<div id="layer">
	[(#REM) Angoli tondi]
	<div id="tl"></div>
	<div id="tr"></div>
  	<div id="bl"></div>
  	<div id="br"></div>
	[(#REM) Intestazione ]
	<div id="header">
		<INCLURE{fond=inc-title}{id_rubrique}{id_secteur}>
	</div><!-- div header -->

	[(#REM) Colonna laterale ]
	<div id="contents">
		<div id="colonna_principale">
			<div id="corpo-colonna_principale">

				<div id="gerarchia">
					<a href="#URL_SITE_SPIP/"><:accueil_site:></a><BOUCLE_ariane(HIERARCHIE){id_article}> &gt; <a href="#URL_RUBRIQUE">[(#TITRE|couper{80})]</a></BOUCLE_ariane>
				</div><!-- div gerarchia -->

				[(#REM) Testo Articolo]
				<div id="articolo_contents">

					<div id="rubrica_intestazione">
						<h2 id="articolo_titolo">#TITRE</h2>
						<B_autori><p><small><:par_auteur:> <BOUCLE_autori(AUTEURS){id_article}{", "}><a href="#URL_AUTEUR" title="#NOM">#NOM</a></BOUCLE_autori></small></p></B_autori>
						<BOUCLE_logo_articolo(DOCUMENTS){id_article}{titre=logo_880x300}>
							<INCLURE{fond=share_utility}>
							<hr class="sommario_ultimi" />
							[(#URL_DOCUMENT|image_recadre{880,300,center}|inserer_attribut{class,articolo_logo_880x300})]
							<B_tags_1><div id="tags" style="border: 1px solid #CCC" title="<:mots_clefs:>"><BOUCLE_tags_1(MOTS) {id_article}{" | "}><a href="#URL_MOT" title="#TITRE">#TITRE</a></BOUCLE_tags_1></div></B_tags_1>
							<hr class="sommario_ultimi" />
							[<p><:voir_en_ligne:>: <a href="(#URL_SITE)">[(#NOM_SITE|sinon{[(#URL_SITE|couper{80})]})]</a></p>]
						</BOUCLE_logo_articolo>
							[(#LOGO_ARTICLE|left|image_passe_partout{250}|inserer_attribut{class,sommario_ultimi})]
							<div id="articolo_utility" style="float: right; width: 600px; font-size: 11px;">
								<INCLURE{fond=share_utility}>
								<hr class="sommario_ultimi" />
								<B_tags><div id="tags" style="border: 1px solid #CCC;" title="<:mots_clefs:>"><BOUCLE_tags(MOTS) {id_article}{" | "}><a href="#URL_MOT" title="#TITRE">#TITRE</a></BOUCLE_tags></div></B_tags>
								<hr class="sommario_ultimi" />
								[<p><:voir_en_ligne:>: <a href="(#URL_SITE)">[(#NOM_SITE|sinon{[(#URL_SITE|couper{80})]})]</a></p>]
								</div><!-- div articolo_utility -->
							<hr class="sommario_ultimi" />

						<//B_logo_articolo>
					</div><!-- div rubrica_intestazione" -->


					[(#CHAPO)]

					<div style="clear:both"></div>

					[(#TEXTE|image_reduire{320,0})]
					<script type="text/javascript">
					$("dl.spip_documents dt a").fancybox({
						'hideOnContentClick': true,
						'overlayShow': true
					});
					</script>
				</div><!-- div articolo_contents -->

				[(#REM) Portfolio]
<B_portfolio_immagini>
				<hr />
				<h3><:info_portfolio:></h3>
<BOUCLE_portfolio_immagini(DOCUMENTS) {id_article} {mode=document} {titre!==^logo\_} {extension IN png,jpg,gif} {par num titre, date} {doublons} {vu=non}>
					[<a class="portfolio" href="(#URL_DOCUMENT)" rel="img_portfolio" type="#MIME_TYPE" [ class="(#EXPOSER)"][ title="(#TITRE|attribut_html|couper{80})"]>	[(#FICHIER|image_passe_partout{100}|image_recadre{100,100,center}|inserer_attribut{alt,[(#TITRE|attribut_html|couper{80})]})]</a>]
</BOUCLE_portfolio_immagini>
		<script type="text/javascript">
			$("a.portfolio").fancybox({
				'hideOnContentClick': true,
				'overlayShow': true
			});
		</script>
</B_portfolio_immagini>

				[(#REM) Altri documenti ]
<B_documenti>
				<hr />
				<h3><:titre_documents_joints:></h3>
				<ul>
<BOUCLE_documenti(DOCUMENTS) {id_article} {mode=document} {titre!==^logo\_} {par num titre, date} {doublons}>
					<li>
						<div>
							[(#LOGO_DOCUMENT|#URL_DOCUMENT)]<br />
							<a href="#URL_DOCUMENT"[ title="#TITRE"] type="#MIME_TYPE">#TITRE</a><br />
							<small>(#TYPE_DOCUMENT[ - (#TAILLE|taille_en_octets)])</small>
						</div>
						[<div>(#DESCRIPTIF)</div>]
					</li>
</BOUCLE_documenti>
				</ul>
</B_documenti>

				[<hr />
				<h3><:info_notes:></h3>
				(#NOTES)]

				<p class="disclaimer">(pubblicato il [(#DATE|affdate_court)][ &mdash; modificato il (#DATE_MODIF|affdate_court|!={[(#DATE|affdate_court)]}|?{[(#DATE_MODIF|affdate_court)]})])</p>
			</div><!-- div corpo-colonna_principale -->
		</div><!-- div colonna_principale -->

		<div id="colonna_laterale_dx">
			<div id="corpo-colonna_laterale_dx">
					<INCLURE{fond=inc_googleadsense_skyscraperlargo}>
			</div><!-- div corpo-colonna_laterale_dx -->
		</div><!-- div colonna_laterale_dx -->
	</div><!-- div contents -->

	[(#REM) Pie di Pagina ]
	<div id="footer">
		<INCLURE{fond=inc-footer}{skel=#SQUELETTE}>
	</div><!-- div footer -->
</div><!-- div layer -->
</body>
</html>
</BOUCLE_principale>
